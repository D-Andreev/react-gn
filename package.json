{
  "name": "@react-gn/react-gn",
  "version": "1.0.27",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "react-gn": "./dist/index.js"
  },
  "keywords": [
    "react",
    "cli",
    "generator"
  ],
  "scripts": {
    "build": "tsc",
    "postbuild": "node ./scripts/postbuild.js",
    "watch-ts": "tsc -w",
    "lint": "tsc --noEmit && eslint \"src/**/*.ts\"",
    "test": "jest --forceExit --coverage",
    "test-unit": "npm run test ./test/unit/",
    "test-unit-ci": "export TEST_ENV=CI && npm run test ./test/unit/",
    "test-feature": "npm run test ./test/feature/",
    "test-feature-ci": "export TEST_ENV=CI && sudo npm link && npm run test ./test/feature/",
    "watch-test": "npm run test -- --watchAll",
    "postpublish": "npm run postbuild"
  },
  "devDependencies": {
    "@types/ejs": "^3.0.0",
    "@types/inquirer": "^6.5.0",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.12.17",
    "@types/prettier": "^1.19.0",
    "@types/rimraf": "^2.0.3",
    "@types/steed": "^1.1.2",
    "@typescript-eslint/eslint-plugin": "^2.2.0",
    "@typescript-eslint/parser": "^2.2.0",
    "eslint": "^6.4.0",
    "jest": "^24.9.0",
    "ts-jest": "^24.1.0",
    "typescript": "^3.6.3"
  },
  "dependencies": {
    "ejs": "^3.0.1",
    "inquirer": "^7.0.0",
    "inquirer-path": "^1.0.0-beta5",
    "ncp": "^2.0.0",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "steed": "^1.1.3"
  }
}
