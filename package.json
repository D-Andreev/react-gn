{
  "name": "@react-gn/react-gn",
  "version": "1.0.11",
  "preinstall": "npm i -g create-react-app",
  "main": "dist/index.js",
  "types" : "dist/index.d.ts",
  "bin": {
    "react-gn": "./dist/index.js"
  },
  "keywords": [
    "react",
    "cli",
    "generator"
  ],
  "scripts": {
    "build": "tsc",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "lint": "tsc --noEmit && eslint \"src/**/*.ts\"",
    "test": "jest --forceExit --coverage",
    "test-ci": "export TEST_ENV=CI && sudo npm link && yarn test",
    "test-unit": "npm run test ./test/unit/",
    "test-feature": "npm run test ./test/feature/",
    "watch-test": "npm run test -- --watchAll"
  },
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.5",
    "@typescript-eslint/eslint-plugin": "^2.2.0",
    "@typescript-eslint/parser": "^2.2.0",
    "eslint": "^6.4.0",
    "jest": "^24.9.0",
    "npm": "^6.11.3",
    "ts-jest": "^24.1.0",
    "typescript": "^3.6.3"
  }
}
